<%= form_tag(sessions_path, :class => "active")  do %>
<div class="row">
  <div class="col s4 push-s2">
    <%= text_field_tag :username %>
    <%= label_tag :username %>
  </div>
  <div class="col s4 push-s2">
    <%= password_field_tag :password %>
    <%= label_tag :password %>
  </div>
</div>
<div class="row">
  <div class="col s4 push-s4">
    <%= submit_tag "Log In", class: "waves-effect waves-light btn"  %>
  </div>
</div>
<% end %>
